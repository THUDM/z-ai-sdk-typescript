{
  "name": "z-ai-sdk-typescript",
  "version": "1.0.0",
  "description": "TypeScript SDK for ZAI API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "types": {
      "require": "./dist/index.d.cts",
      "default": "./dist/index.d.ts"
    },
    "default": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/** --splitting false --format cjs,esm --sourcemap --dts --clean",
    "dev": "tsup src/** --splitting false --format cjs,esm --sourcemap --dts --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "zai",
    "ai",
    "sdk",
    "typescript",
    "openai",
    "chat",
    "completion"
  ],
  "author": "ZAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zai/z-ai-sdk-typescript"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
